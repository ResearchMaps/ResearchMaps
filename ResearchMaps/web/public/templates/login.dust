<!-- Main Container -->
<section>
<div class="container login">
	<div class="row">
		<div class="col-xs-4 col-xs-offset-4 col-sm-4 col-sm-offset-4 col-md-4 col-md-offset-4 col-lg-4 col-lg-offset-4 well well-lg">
			<legend style="color: #690C15;">Sign In</legend>
			<div class="alert alert-error hide" id="incorrect">
				<a class="close" data-dismiss="alert" href="#">Ã—</a>Incorrect Username or Password!
			</div>
			<form id="frmLogin" method="POST" class="form" action="/login" accept-charset="UTF-8" role="form">
				<div class="form-group">
					<label for="username" class="" style="color: #690C15;">Username</label>
					<div class="">
						<input type="text" id="username" class="form-control" name="username" placeholder="Username" />
					</div>
				</div>
				<div class="form-group">
					<label for="password" class="" style="color: #690C15;">Password</label>
					<div class="">
						<input type="password" id="password" class="form-control" name="password" placeholder="Password" />
					</div>
				</div>
				<input type="hidden" name="_csrf" value="{_csrf}">
				<div class="form-group">
					<div class="">
						<label class="checkbox">
							<input type="checkbox" name="remember" value="1" /> Remember Me
						</label>
					</div>
				</div>
				<div class="form-group">
					<div class="">
						<a href="#" onclick="Maps.modal('forgot')" data-toggle="modal" data-target="#sforgot">Forgot Password?</a>
					</div>
				</div>
				<div class="form-group">
					<div class="">
						<button onclick="Maps.Hacks.checkPassword(this); return false;" class="btn btn-primary">Sign in</button>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>

</section>
