<div id="forgot" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="forgot" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
				<h3 class="modal-title">Forgot Password</h3>
			</div>
			<div class="modal-body">
				<p>Enter your username to reset the password</p>
				<form>
					<div class="controls controls-row">
						<input id="forgotUsername" name="name" type="text" class="span3 form-control" placeholder="Name" />
					</div>
 
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" id="forgotPassword" href="#" class="btn btn-primary">Submit</button>
				<button type="button" href="#" class="btn btn-default" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>

